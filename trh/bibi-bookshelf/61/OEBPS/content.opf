    <dc:title>Sapiens</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Harari, Yuval Noah">Yuval Noah Harari</dc:creator>
    <dc:date>2015-03-17T22:00:00+00:00</dc:date>
